{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "cookie",
    "description": "#### api documentation for  [cookie (v0.3.1)](https://github.com/jshttp/cookie)  [![npm package](https://img.shields.io/npm/v/npmdoc-cookie.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-cookie) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-cookie.svg)](https://travis-ci.org/npmdoc/node-npmdoc-cookie)",
    "devDependencies": {
        "cookie": "*",
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-cookie",
    "keywords": [
        "documentation",
        "cookie"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_cookie.js",
    "name": "npmdoc-cookie",
    "nameAlias": "npmdoc_cookie",
    "nameOriginal": "npmdoc-cookie",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-cookie"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
